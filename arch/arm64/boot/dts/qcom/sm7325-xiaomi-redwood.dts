&pm7325_gpios {
	volume_down_default: volume-down-default-state {
		pins = "gpio6";
		function = PMIC_GPIO_FUNC_NORMAL;
		power-source = <1>;
		bias-pull-up;
		input-enable;
	};
};

&pm8008_chip {
	pinctrl-names = "default";
	pinctrl-0 = <&pm8008_active>;
};

&pm8008_regulators {
	vdd_l1_l2-supply = <&vreg_s8b>;
	vdd_l3_l4-supply = <&vreg_bob>;
	vdd_l5-supply = <&vreg_bob>;
	vdd_l6-supply = <&vreg_s1b>;
	vdd_l7-supply = <&vreg_bob>;
};

&pm8008_l1 {
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1200000>;
	regulator-min-dropout-voltage-microvolt = <160000>;
	regulator-always-on; // FIXME
};

&pm8008_l2 {
	regulator-min-microvolt = <950000>;
	/* Adjusted from downstream 1150000 to avoid regulator core failure
	   since current_uV on boot is 1.2V, but the regulator steps don't
	   include exactly 1.15V, just 1.152V */
	regulator-max-microvolt = <1152000>;
	regulator-min-dropout-voltage-microvolt = <128000>;
	regulator-always-on; // FIXME
};

&pm8008_l3 {
	regulator-min-microvolt = <2700000>;
	regulator-max-microvolt = <3000000>;
	regulator-min-dropout-voltage-microvolt = <144000>;
	regulator-always-on; // FIXME
};

&pm8008_l4 {
	regulator-min-microvolt = <2700000>;
	regulator-max-microvolt = <2900000>;
	regulator-min-dropout-voltage-microvolt = <128000>;
	regulator-always-on; // FIXME
};

&pm8008_l5 {
	regulator-min-microvolt = <1700000>;
	regulator-max-microvolt = <1900000>;
	regulator-min-dropout-voltage-microvolt = <128000>;
	regulator-always-on; // FIXME
};

&pm8008_l6 {
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <1800000>;
	regulator-min-dropout-voltage-microvolt = <136000>;
	regulator-always-on; // FIXME CCI pull-up regulator
};

&pm8008_l7 {
	regulator-min-microvolt = <2700000>;
	regulator-max-microvolt = <3000000>;
	regulator-min-dropout-voltage-microvolt = <312000>;
	regulator-always-on; // FIXME
};

&pm8350c_flash {
	status = "okay";

	led-0 {
		function = LED_FUNCTION_FLASH;
		color = <LED_COLOR_ID_WHITE>;
		led-sources = <1>, <4>;
		led-max-microamp = <500000>;
		flash-max-microamp = <1500000>;
		flash-max-timeout-us = <1280000>;
	};
};

&pm8350c_gpios {
	pm8008_active: pm8008-active-state {
		pins = "gpio3";
		function = PMIC_GPIO_FUNC_NORMAL;
		bias-disable;
		output-high;
	};
};

&pmk8350_adc_tm {
	status = "okay";

	xo-therm@0 {
		reg = <0>;
		io-channels = <&pmk8350_vadc PMK8350_ADC7_AMUX_THM1_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time-us = <200>;
	};

	quiet-therm@1 {
		reg = <1>;
		io-channels = <&pmk8350_vadc PM7325_ADC7_AMUX_THM1_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time-us = <200>;
	};

	cam-flash-therm@2 {
		reg = <2>;
		io-channels = <&pmk8350_vadc PM7325_ADC7_AMUX_THM2_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time-us = <200>;
	};

	sdm-skin-therm@3 {
		reg = <3>;
		io-channels = <&pmk8350_vadc PM7325_ADC7_AMUX_THM3_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time-us = <200>;
	};

	wide-rfc-therm@4 {
		reg = <4>;
		io-channels = <&pmk8350_vadc PM7325_ADC7_AMUX_THM4_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time-us = <200>;
	};
};

&pmk8350_rtc {
	status = "okay";
};

&pmk8350_vadc {
